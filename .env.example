# MedSynEval Environment Configuration
# Copy this file to .env and update with your actual values

# Django Settings
SECRET_KEY=your-secret-key-here-change-this-in-production
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Configuration
# Format: mysql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL=mysql://root:your_mysql_password@localhost:3306/med_syn_eval

# Example with custom user:
# DATABASE_URL=mysql://medsyneval:your_password@localhost:3306/med_syn_eval

# Example for production with SSL:
# DATABASE_URL=mysql://user:password@host:3306/database?ssl-ca=/path/to/ca-cert.pem

# Media Files (optional - defaults are fine for development)
# MEDIA_ROOT=/path/to/media
# MEDIA_URL=/media/
